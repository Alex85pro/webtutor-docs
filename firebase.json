{
  "hosting": {
    "public": "vuepress",
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }],
    "headers": [
      {
        "source": "**/service-worker.js",
        "headers": [
          {
            "key": "Service-Worker-Allowed",
            "value": "/"
          }
        ]
      },
      {
        "source": "**/*.js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript"
          }
        ]
      }
    ]
  }
}
